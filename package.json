{
  "name": "pixl",
  "version": "0.13.2",
  "description": "Front-end web library",
  "scripts": {
    "build": "node-sass --include-path scss src/scss/main.scss dist/css/pixl.css && npm run beautify-css && npm run minify-css",
    "beautify-css": "beautify-css dist/css/pixl.css dist/css/pixl.css",
    "minify-css": "minify dist/css/pixl.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justtoconfirm/pixl.git"
  },
  "author": "Justtoconfirm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justtoconfirm/pixl/issues"
  },
  "homepage": "https://github.com/justtoconfirm/pixl#readme",
  "devDependencies": {
    "beautify-css": "^1.0.0",
    "minifier": "^0.8.1",
    "node-sass": "^4.7.2"
  }
}
